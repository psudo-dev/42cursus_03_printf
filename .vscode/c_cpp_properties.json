{
  "configurations": [
    {
      "name": "macos-clang-x64",
      "includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/inc",
        "${workspaceFolder}/lib/libft/inc"
      ],
      "defines": [],
      "macFrameworkPath": [
        "/System/Library/Frameworks",
        "/Library/Frameworks"
      ],
      "compilerPath": "/usr/bin/clang",
      "cStandard": "c11",
      "cppStandard": "c++98",
      "intelliSenseMode": "macos-clang-x64",
      "configurationProvider": "ms-vscode.makefile-tools",
      "compilerArgs": [
        "-Wall",
        "-Wextra",
        "-Werror"
      ]
    }
  ],
  "version": 4
}
